{
  "name": "@ivobubenko/chained-qr-reader",
  "version": "1.0.7",
  "description": "Library for secure QR code scanning in an isolated environment.",
  "main": "./dist/secure-qr-scanner.umd.js",
  "module": "./dist/secure-qr-scanner.es.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/secure-qr-scanner.es.js",
      "require": "./dist/secure-qr-scanner.umd.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint \"src/**/*.js\" \"tests/**/*.js\"",
    "test": "vitest run"
  },
  "keywords": [
    "qr",
    "qr-code",
    "scanner",
    "secure",
    "webworker",
    "library"
  ],
  "author": "Ivo Bubenko",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivobubenko/chained-qr-reader.git"
  },
  "dependencies": {
    "@zxing/browser": "^0.1.5",
    "cbor-x": "^1.6.0",
    "pako": "^2.1.0",
    "rollup-plugin-web-worker-loader": "^1.7.0"
  },
  "devDependencies": {
    "eslint": "^9.13.0",
    "rollup": "^4.40.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "vitest": "^2.1.4"
  }
}
